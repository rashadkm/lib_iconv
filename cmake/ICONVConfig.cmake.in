# Compute paths
get_filename_component(ICONV_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(ICONV_INCLUDE_DIR "@CONF_INCLUDE_DIRS@" PARENT_SCOPE)

set(BUILD_SHARED_LIBS @BUILD_SHARED_LIBS@)

if(NOT TARGET @LIB_NAME@ AND NOT ICONV_BINARY_DIR)
  include("${ICONV_CMAKE_DIR}/ICONVTargets.cmake")
endif()

if(MSVC)
  if(BUILD_SHARED_LIBS)
    get_target_property(LIB_LIB_FILENAME @LIB_NAME@ IMPORTED_IMPLIB_@CONFIG_TYPE@)
  else(BUILD_SHARED_LIBS)
    get_target_property(LIB_LIB_FILENAME @LIB_NAME@ IMPORTED_LOCATION_@CONFIG_TYPE@)
  endif(BUILD_SHARED_LIBS)
else()
  get_target_property(LIB_LIB_FILENAME @LIB_NAME@ IMPORTED_LOCATION_@CONFIG_TYPE@)
endif()

set(ICONV_LIBRARY ${LIB_LIB_FILENAME} PARENT_SCOPE)
#compatiblity
set(ICONV_LIBRARIES ${LIB_LIB_FILENAME} PARENT_SCOPE)

set(ICONV_FOUND TRUE)
